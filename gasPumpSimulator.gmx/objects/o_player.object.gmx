    spd = 4;
    lenX = lengthdir_x(spd,image_angle+45)
    lenY = lengthdir_y(spd,image_angle+45)
    if ( place_free(x+lenX,y+lenY) )
    {
      x+=lenX;
      y+=lenY;
      moved = true;
    }
  }
}
if(moved == true)
{
